{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Next.js",
	"extends": "./base.json",
	"compilerOptions": {
		"jsx": "preserve",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"noEmit": true,
		"plugins": [{ "name": "next" }],
		"lib": ["DOM", "DOM.Iterable", "ESNext"]
	}
}
