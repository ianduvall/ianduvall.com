{
	"schema": "https://json.schemastore.org/tsconfig",
	"extends": "tsconfig/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"plugins": [
			{
				"name": "next"
			}
		],
		"allowJs": true
	},
	"include": [
		".next/types/**/*.ts",
		"**/*.ts",
		"**/*.tsx",
		"next-env.d.ts",
		"next.config.ts"
	],
	"exclude": ["**/node_modules", "**/.*/"]
}
